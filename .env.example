# Myfxbook API Credentials
# Copy this file to .env.local and fill in your credentials
# DO NOT commit .env.local to version control

MYFXBOOK_EMAIL=your-email@example.com
MYFXBOOK_PASSWORD=your-password

# SOCKS5 or HTTP proxy
HTTPS_PROXY=http://user:password@exampleproxy.com:port
HTTP_proxy=http://user:password@exampleproxy.com:port
